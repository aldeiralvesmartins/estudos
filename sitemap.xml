    function inject_r2_debug_assets_once(): void
    {
        static $injected = false;
        if ($injected) return;
        $injected = true;

        echo <<<HTML
<style id="r2dbg-style">
  .r2dbg{margin:1rem 0;border:1px solid #262a35;border-radius:14px;background:#0e1117;color:#e6e6e6;font:14px ui-sans-serif,system-ui,-apple-system,"Segoe UI",Roboto,Ubuntu,"Noto Sans",Arial;}
  .r2dbg *{box-sizing:border-box}
  .r2dbg__header{position:sticky;top:0;z-index:1;display:grid;grid-template-columns:1fr auto;gap:.5rem;align-items:center;padding:.8rem 1rem;border-bottom:1px solid #1b2130;background:linear-gradient(180deg,#121726,#0f1420)}
  .r2dbg__title{display:flex;gap:.5rem;align-items:baseline}
  .r2dbg__label{opacity:.9;font-weight:600;color:#b9c2ff}
  .r2dbg__meta{display:flex;gap:.6rem;align-items:center;flex-wrap:wrap;opacity:.9}
  .r2dbg__crumbs code{background:#121825;padding:.15rem .35rem;border-radius:6px}
